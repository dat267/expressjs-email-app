<link rel="stylesheet" href="/styles/header.css">
<header>
  <nav id="top-navbar">
    <% if(typeof user !='undefined' && user) { %>
      <span>
        <%= user.email %>
      </span>
      <a href="/inbox">Inbox</a>
      <a href="/outbox">Outbox</a>
      <a href="/compose">Compose</a>
      <a href="/signout">Sign Out</a>
      <% } else { %>
        <a href="/">Sign in</a>
        <a href="/signup">Sign up</a>
        <% } %>
  </nav>
</header>
